# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           	controllers.Application.index()
# Main pages
GET     /home                           controllers.Application.homePage()
GET 	/profSignIn						controllers.Application.profSignIn()
GET		/profMainPage					controllers.Application.profMainPage()
POST	/postQuestion					controllers.Application.postQuestion()
GET  	/indexStudent   				controllers.Application.indexStudent()
GET  	/vote    						controllers.Application.vote()
GET 	/indexStudent					controllers.Application.indexStudent()
GET 	/profPage						controllers.Application.profPageDisplay()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               	controllers.Assets.at(path="/public", file)


